#!/usr/bin/env loli

import sys
import (parse_route, parse_args) utils
import views

var request_line = input().to_s()
var request_url = request_line.split()[1]

var route = parse_route(request_url)
var args = parse_args(request_url)

if route == "quiz": {
    views.quiz(args)
elif route == "postquiz":
    views.postquiz(args)
elif route == "createquiz":
    views.createquiz(args)
else:
    views.main(args)
}